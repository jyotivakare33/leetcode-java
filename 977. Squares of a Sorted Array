class Solution {
    public int[] sortedSquares(int[] nums) {
        int[] output = new int[nums.length];
        int count = 0;
        for(int i = 0; i <= nums.length - 1; i++) {
            output[count] = nums[i] * nums[i];
            count++;
        }
        Arrays.sort(output); 
        return output;
    }
}
